{
  "name": "orders-manager-frontend",
  "homepage": "https://dimadk24.github.io/orders-manager-frontend",
  "description": "app for managing orders in online store",
  "repository": {
    "type": "git",
    "url": "https://github.com/DimaDK24/orders-manager-frontend.git"
  },
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "classnames": "^2.2.6",
    "moment": "^2.24.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-datetime": "^2.16.3",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write \"./**/*.{js,jsx,json,css}\" \"!./build/**\"",
    "lint:js": "eslint \"src/**/*.{js,jsx}\"",
    "lint:css": "stylelint \"src/**/*.{css,sass,scss}\"",
    "lint": "yarn lint:js && yarn lint:css",
    "deploy": "gh-pages -d build",
    "predeploy": "yarn build",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.1.10",
    "@storybook/addon-links": "^5.1.10",
    "@storybook/addon-notes": "^5.1.10",
    "@storybook/addons": "^5.1.10",
    "@storybook/react": "^5.1.10",
    "eslint": "~5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.14.0",
    "eslint-plugin-jest-formatting": "^1.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "gh-pages": "^2.0.1",
    "prettier": "1.18.2",
    "stylelint": "~10.1.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-prettier": "^1.1.1"
  },
  "keywords": [
    "react"
  ],
  "license": "MIT"
}
